{
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "src": "^/$",
      "dest": "/index.html"
    },
    {
      "src": "^/auth$",
      "dest": "/auth.html"
    },
    {
      "src": "^/dashboard$",
      "dest": "/01-pages/01-dashboard.html"
    },
    {
      "src": "^/products$",
      "dest": "/01-pages/02-products.html"
    },
    {
      "src": "^/products-calendar$",
      "dest": "/01-pages/03-products-calendar.html"
    },
    {
      "src": "^/delivery$",
      "dest": "/01-pages/04-delivery-calendar.html"
    },
    {
      "src": "^/orders$",
      "dest": "/01-pages/05-orders.html"
    },
    {
      "src": "^/services$",
      "dest": "/01-pages/06-services.html"
    },
    {
      "src": "^/notice$",
      "dest": "/01-pages/07-notice.html"
    },
    {
      "handle": "filesystem"
    }
  ],
  "headers": [
    // 기존 headers 설정 그대로 유지
  ]
}
